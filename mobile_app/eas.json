{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://192.168.1.132:8000"
      },
      "ios": { "simulator": false }
    },
    "preview": {
      "channel": "staging",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://gapptomobile-api-v3-staging.onrender.com/"
      },
      "ios": { "simulator": false }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "https://gapptomobile-api-v3-staging.onrender.com/"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
